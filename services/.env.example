ENVIRONMENT="development"

# CORS settings
CORS_ENABLED="true"
CORS_TRUSTED_ORIGINS="127.0.0.1:8080;"

# Database Connection Settings
DB_CONNECTION_STRING="postgres://user:abcd1234@localhost:5432/dbName?sslmode=disable"
DB_MAX_IDLE_CONNS=2
DB_MAX_OPEN_CONNS=5

# Database Timeout Settings
DB_CONN_MAX_IDLE_TIME=60
DB_CONN_MAX_LIFETIME=300

# Rate Limiter Settings
RATE_LIMIT_ENABLED="false"
RATE_LIMIT_BURST=3
RATE_LIMIT_RPS=3

# Server Settings
HTTP_HOST="0.0.0.0"
HTTP_PORT=8080

HTTPS_CERTIFICATE_FILE_PATH=""
HTTPS_CERTIFICATE_KEY_FILE_PATH=""
SERVER_IDLE_TIMEOUT=120
SERVER_READ_TIMEOUT=5
SERVER_WRITE_TIMEOUT=2